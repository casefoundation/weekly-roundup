  "dependencies": {
    "axios": "^0.21.1",
    "bluebird": "^3.5.1",
    "bootstrap": "^4.0.0-beta.2",
    "classnames": "^2.2.5",
    "dateformat": "^3.0.3",
    "dot-prop-immutable": "^1.4.0",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "0.0.3",
    "normalizr": "^3.2.4",
    "popper": "^1.0.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-fontawesome": "^1.6.1",
    "react-js-pagination": "^3.0.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.0.17",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^5.0.0-alpha.4",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-equimper": "^2.2.1",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "jshint": "^2.9.5",
    "npm-run-all": "^4.1.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/lib/scss/style.scss -o src/lib/css/ --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar src/lib/scss/style.scss -o src/lib/css/ --watch --recursive --output-style compressed",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:8000"
}
